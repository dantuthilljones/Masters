<ServerManagerConfiguration>
  <ProxyGroup name="sources">
   <SourceProxy name="PeanoReader"
                class="vtkPeanoReader"
                label="Peano reader">
     <Documentation
       short_help="Read a Peano file."
       long_help="Read a single Peano file.">
       The Peano reader reads Peano data file.
     </Documentation>
     <StringVectorProperty
        name="FileName"
        animateable="0"
        command="SetFileName"
        number_of_elements="1">
        <FileListDomain name="files"/>
        <Documentation>
          This property specifies the file name for the Peano reader.
        </Documentation>
     </StringVectorProperty>
     <DoubleVectorProperty information_only="1"
                          name="TimestepValues"
                          repeatable="1">
      <TimeStepsInformationHelper />
      <Documentation>Available timestep values.</Documentation>
    </DoubleVectorProperty> 
     <Hints>
       <ReaderFactory extensions="peano-patch-file"
                      file_description="Peano Patch File Format" />
     </Hints>
   </SourceProxy>
  </ProxyGroup>


</ServerManagerConfiguration>
